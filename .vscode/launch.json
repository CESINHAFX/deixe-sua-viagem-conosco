
{
  "version": "0.2.0",
  "configurations": [
    
    {
      "type": "node",
      "request": "launch",
      "name": "Launch npm start",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run",
        "start"
      ],
      "cwd": "${workspaceFolder}/project_root",
      "internalConsoleOptions": "neverOpen"
    },
    {
      "type": "msedge",
      "request": "launch",
      "name": "Launch Edge com Live Server",
      "url": "http://localhost:5500",
      "webRoot": "${workspaceFolder}"
    },
    {
      "type": "pwa-msedge",
      "request": "launch",
      "name": "Launch Edge - project_root (http)",
      "url": "http://localhost:5500/index.html",
      "webRoot": "${workspaceFolder}/project_root",
      "runtimeArgs": []
    }
  ]
}
